a:hover.gaim{background:#D0C6AC url(../pixels/sidebaricons/sidemenu_back_gaim.jpg) no-repeat right;}

a.tinyclose{float:right;margin:8px;font-size:11px;color:#9A623D;padding:2px;text-decoration:none;display:inline;border:1px solid #F7E9D0;}
a:hover.tinyclose{background:#fff;text-decoration:none;border:1px solid #D0C6AC;}
a.tinyclose em{display:none;}
a:hover.tinyclose em{display:inline !important;background:#fff;text-decoration:none;font-style:normal !important;padding-left:3px;}






/** About Box**/
/** #aboutboxbelow{border-top:3px solid #655F56;border-right:3px solid #655F56;float:left;margin:0px;width:590px;background:#5F6968 url(../pixelography/aboutbox_background.gif) repeat-x -4px;padding:0px;} **/

#aboutboxbelow{float:left;margin:0px;width:590px;padding:0px;}
#aboutboxbelow span{float:left;width:250px;border-right:1px dotted #D9CFB1;padding-left:15px;padding-right:15px;padding-top:30px;}
#aboutboxbelow p{color:#B95E52;font-size:14px;line-height:17px;text-align:left;margin-bottom:10px;}
#aboutboxbelow strong{color:#655F56;}
#aboutboxbelow h3{font-size:16px;text-align:center;margin:0px;padding:3px;border-bottom:1px solid #655F56;}
#aboutboxbelow a{color:#899796;}
#aboutboxbelow a:hover{color:#655F56;text-decoration:none;}
#aboutboxbelow ul{padding:0;list-style: none;margin: 0px 0px 20px;}
#aboutboxbelow li{font-size:14px;text-align:left;line-height:17px;margin-left:8px;padding-top:7px;padding-bottom:7px;padding-left:15px;border-bottom:1px dotted #D9CFB1;}
#aboutboxbelow li.alt{background:#F7F6F7;font-size:14px;text-align:left;line-height:17px;margin-left:8px;padding-top:7px;padding-bottom:7px;padding-left:15px;border-bottom:1px dotted #D9CFB1;}
#aboutboxbelow li.noborder{border-bottom:none !important;}
#aboutboxbelow img{margin-left:14px;margin-top:10px;}

#aboutclose{float:left;clear:right;padding-left:20px;}
#aboutclose a{display:block;border:1px solid #899796;background:#E0E3E2;color:#655F56;text-align:center;line-height:15px;margin-bottom:5px;padding:5px;}
#aboutclose a:hover{border:1px solid #fff;background:#899796;color:#fff;}
#aboutboxbelowholder,#moreinfoproj1,#moreinfoproj3,#moreinfoproj2 {height: 0;overflow: hidden;}
